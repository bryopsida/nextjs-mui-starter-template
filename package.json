    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "argon2": "^0.40.0",
    "better-sqlite3": "^10.0.0",
    "drizzle-orm": "^0.30.0",
    "next-auth": "^4.24.5",
    "pino": "^9.0.0",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.44.1",
    "@testing-library/jest-dom": "^6.4.5",
